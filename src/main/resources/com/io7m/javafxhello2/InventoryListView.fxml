<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.ProgressBar?><?import javafx.scene.image.Image?><?import javafx.scene.image.ImageView?><?import javafx.scene.layout.AnchorPane?><?import javafx.scene.text.Font?>
<AnchorPane fx:id="listViewContainer"
            prefHeight="128.0"
            prefWidth="640.0"
            xmlns="http://javafx.com/javafx/8.0.162-ea"
            xmlns:fx="http://javafx.com/fxml/1">

  <fx:define>
    <Image fx:id="buttonImage"
           url="@dialog-error.png"/>
  </fx:define>

  <children>
    <Label fx:id="fileName"
           layoutX="14.0"
           layoutY="14.0"
           prefWidth="606.0"
           text="Label"
           AnchorPane.leftAnchor="16.0"
           AnchorPane.rightAnchor="16.0"
           AnchorPane.topAnchor="16.0">
      <font>
        <Font size="18.0"/>
      </font>
    </Label>
    <Label fx:id="status"
           layoutX="17.0"
           layoutY="48.0"
           prefWidth="606.0"
           text="Label"
           AnchorPane.leftAnchor="16.0"
           AnchorPane.rightAnchor="16.0"
           AnchorPane.topAnchor="48.0"/>
    <ProgressBar fx:id="progress"
                 layoutX="16.0"
                 layoutY="80.0"
                 prefHeight="32.0"
                 prefWidth="200.0"
                 progress="0.0"
                 AnchorPane.bottomAnchor="16.0"
                 AnchorPane.leftAnchor="16.0"
                 AnchorPane.rightAnchor="64.0"/>
    <Button fx:id="stopOrResume"
            contentDisplay="GRAPHIC_ONLY"
            layoutX="594.0"
            layoutY="80.0"
            mnemonicParsing="false"
            prefHeight="32.0"
            prefWidth="32.0"
            AnchorPane.bottomAnchor="16.0"
            AnchorPane.rightAnchor="16.0">
      <graphic>
        <ImageView image="$buttonImage"/>
      </graphic>
    </Button>
  </children>
</AnchorPane>
